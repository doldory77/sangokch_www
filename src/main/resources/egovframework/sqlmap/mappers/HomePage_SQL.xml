<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="HomePage">

	<typeAlias alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>

    <select id="selectMenu" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
        <![CDATA[
            SELECT MENU_ID
            	 , MENU_NM
              FROM H_MENU 
             WHERE USE_YN = 'Y'
               AND MENU_LEVEL = #MENU_LEVEL#
        ]]>
    </select>
    
    <procedure id="selectMenuBySP" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
    	{ call SP_TEST_MENU(#P_LVL#) }
    </procedure>

</sqlMap>
